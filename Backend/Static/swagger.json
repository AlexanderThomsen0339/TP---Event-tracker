{
    "swagger": "2.0",
    "info": {
      "title": "Event API",
      "version": "1.0.0"
    },
    "paths": {
      "/events": {
        "get": {
          "summary": "Get a list of events",
          "responses": {
            "200": {
              "description": "A list of events",
              "schema": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer"
                    },
                    "name": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/event/{event_id}": {
        "get": {
          "summary": "Get a specific event",
          "parameters": [
            {
              "name": "event_id",
              "in": "path",
              "required": true,
              "type": "integer"
            }
          ],
          "responses": {
            "200": {
              "description": "A specific event",
              "schema": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "integer"
                  },
                  "name": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
  